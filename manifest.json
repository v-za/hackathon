{
    "name": "Codesmith Calendar Popup",
    "version": "1.0",
    "description": "",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
          "resources": [ "data.json" ],
          "matches": [ "https://web-accessible-resources-1.glitch.me/*" ]
        }
    ],
    "browser_action": {
        "default_popup": "index.html",
        "default_title": "Test!"
    }
}